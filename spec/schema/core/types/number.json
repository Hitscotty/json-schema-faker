[
  {
    "description": "number generator",
    "tests": [
      {
        "description": "should handle minimum",
        "schema": {
          "type": "number",
          "minimum": 10
        },
        "type": "number",
        "valid": true
      },
      {
        "description": "should handle maximum",
        "schema": {
          "type": "number",
          "minimum": 20,
          "maximum": 30
        },
        "type": "number",
        "valid": true
      },
      {
        "description": "should handle multipleOf",
        "schema": {
          "type": "number",
          "minimum": 40,
          "maximum": 50,
          "multipleOf": 6
        },
        "type": "number",
        "valid": true
      },
      {
        "description": "should handle exclusiveMinimum",
        "schema": {
          "type": "number",
          "minimum": 60,
          "maximum": 70,
          "multipleOf": 7,
          "exclusiveMinimum": true
        },
        "type": "number",
        "valid": true
      },
      {
          "description": "should handle multipleOf being a decimal",
          "schema": {
              "type": "number",
              "minimum": 84,
              "maximum": 90,
              "multipleOf": 0.8,
              "exclusiveMinimum": true,
              "exclusiveMaximum": true
          },
          "type": "number",
          "valid": true
      },
      {
        "description": "should handle exclusiveMaximum",
        "schema": {
          "type": "number",
          "minimum": 1,
          "maximum": 1.5,
          "exclusiveMaximum": true
        },
        "type": "number",
        "valid": true
      },
      {
        "description": "should handle exclusiveMaximum and exclusiveMinimum",
        "schema": {
          "type": "number",
          "minimum": 1,
          "maximum": 1.5,
          "exclusiveMinimum": true,
          "exclusiveMaximum": true
        },
        "type": "number",
        "valid": true
      },
      {
        "description": "should handle exclusiveMaximum and exclusiveMinimum and multipleOf",
        "schema": {
          "type": "number",
          "minimum": 1,
          "maximum": 1.5,
          "multipleOf": 0.25,
          "exclusiveMinimum": true,
          "exclusiveMaximum": true
        },
        "type": "number",
        "valid": true
      },
      {
        "description": "should handle exclusiveMaximum and exclusiveMinimum and multipleOf being impossible",
        "schema": {
          "type": "number",
          "minimum": 1,
          "maximum": 1.5,
          "multipleOf": 0.5,
          "exclusiveMinimum": true,
          "exclusiveMaximum": true
        },
        "type": "number",
        "valid": false
      },
      {
        "description": "should handle inferred type (when possible)",
        "schema": {
          "maximum": 5
        },
        "type": "number"
      },
      {
        "description": "should generate negative values for negative min and negative max",
        "schema": {
          "type": "number",
          "minimum": -10,
          "maximum": -5
        },
        "valid": true
      },
      {
        "description": "should handle negative minimum and 0 as maximum",
        "schema": {
          "type": "number",
          "minimum": -100,
          "maximum": 0
        },
        "valid": true
      },
      {
        "description": "should handle negative minimum and 1 as maximum",
        "schema": {
          "type": "number",
          "minimum": -100,
          "maximum": 1
        },
        "valid": true
      },
      {
        "description": "should handle -1 as minimum and 1 as maximum",
        "schema": {
          "type": "number",
          "minimum": -1,
          "maximum": 1
        },
        "valid": true
      }
    ]
  }
]
